{
  "name": "@boostercloud/framework-types",
  "version": "1.0.0",
  "description": "Contains Booster types related to the information extracted from the user project",
  "keywords": [
    "framework-types"
  ],
  "author": "Booster Cloud",
  "homepage": "https://booster.cloud",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint --ext '.js,.ts' **/*.ts",
    "lint:fix": "eslint --quiet --fix --ext '.js,.ts' **/*.ts",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "test:types": "npm run test",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" 2>&1",
    "prepack": "tsc -b tsconfig.json"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "dependencies": {
    "@types/graphql": "14.5.0",
    "graphql": "^15.5.1",
    "tslib": "^2.4.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@boostercloud/metadata-booster": "workspace:^1.0.0",
    "@boostercloud/eslint-config": "workspace:^1.0.0",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/child-process-promise": "^2.2.1",
    "@types/faker": "5.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/inflected": "1.1.29",
    "@types/inquirer": "^6.5.0",
    "@types/mustache": "4.1.0",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "^3.2.8",
    "copyfiles": "^2.3.0",
    "faker": "5.1.0",
    "fancy-test": "1.4.3",
    "rewire": "5.0.0",
    "rimraf": "3.0.2",
    "@types/uuid": "8.3.0",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "3.4.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "fast-check": "2.17.0",
    "mocha": "8.4.0",
    "nyc": "^15.1.0",
    "sinon": "9.2.3",
    "sinon-chai": "^3.5.0",
    "typescript": "4.7.4",
    "prettier": "2.3.0",
    "eslint-plugin-unicorn": "~43.0.2",
    "@types/node": "^16.11.7"
  },
  "pnpm": {
    "overrides": {
      "pac-resolver@<5.0.0": ">=5.0.0",
      "underscore@>=1.3.2 <1.12.1": ">=1.12.1",
      "node-fetch@<2.6.7": ">=2.6.7",
      "ws@>=7.0.0 <7.4.6": ">=7.4.6",
      "nanoid@>=3.0.0 <3.1.31": ">=3.1.31",
      "node-fetch@<2.6.1": ">=2.6.1"
    }
  }
}
